		<js-module src='src/sectv-tizen/MediaPluginWideVine.js' name='MediaPluginWideVine'>
			<clobbers target='toast.MediaPluginWideVine' />
		</js-module>
		<js-module src='src/sectv-tizen/MediaPluginHLS.js' name='MediaPluginHLS'>
			<clobbers target='toast.MediaPluginHLS' />
		</js-module>
		<js-module src='src/sectv-tizen/MediaPluginUHD.js' name='MediaPluginUHD'>
